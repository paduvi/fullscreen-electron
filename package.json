{
  "name": "fullscreen-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron-starter.js",
  "productName": "DeathFullScreen",
  "devDependencies": {
    "asar": "^0.13.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.0",
    "foreman": "^2.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-modal-dialog": "^4.0.7",
    "react-scripts": "1.0.7",
    "react-spinjs": "^3.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000 -w",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron . --enable-logging",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-scripts start",
    "package-mac": "react-scripts build && electron-packager . --platform=darwin --overwrite --prune=true --out=dist --ignore=\"(node_modules|.git|dist)\" --icon=./public/icon/mac/logo.icns",
    "package-win": "react-scripts build && electron-packager . --platform=win32 --overwrite --asar=true --prune=true --out=dist --ignore=\"(node_modules|.git|dist)\" --icon=./public/icon/win/logo.ico",
    "package-linux": "react-scripts build && electron-packager . --platform=linux --overwrite --prune=true --out=dist --ignore=\"(node_modules|.git|dist)\" --icon=./public/icon/png/logo.png"
  },
  "dependencies": {
    "electron-sudo": "^4.0.12"
  }
}
